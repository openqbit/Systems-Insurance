
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_System_User_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div style="background-color:#ff0000;" class="panel"></div>
        </div>
        <div class="col-md-4">
            <div style="background-color:#0026ff;" class="panel"></div>
        </div>
        <div class="col-md-6">
            <div style="background-color:#ff6a00;" class="panel"></div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row panel panel-info">
        <div class="panel-heading">
            Client Area
        </div>
        <div class="panel-body">
            <form class="form-horizontal">
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Identity</h5>
                    <div class="col-sm-10">
                        <select class="form-control">
                            <option>-Select-</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Name</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control"  />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Age</h5>
                    <div class="col-sm-10">
                        <input type="number" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Address</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client DOB</h5>
                    <div class="col-sm-10">
                        <input type="date" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Gender</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Nationality</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Reigion</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>

                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Blood Group</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Email</h5>
                    <div class="col-sm-10">
                        <input type="email" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Mobile</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <h5 class="col-sm-2 control-label">Client Tele</h5>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">                    
                        <a style="cursor:pointer;">The Client is New Add New Client</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div style="background-color:#4cff00;" class="panel"></div>
        </div>
        <div class="col-md-4">
            <div style="background-color:#b200ff;" class="panel"></div>
        </div>
        <div class="col-md-6">
            <div style="background-color:#0094ff;" class="panel"></div>
        </div>
    </div>
</div>
<div class="container" style="padding-bottom:15px;">
    <div class="row">
        <div class="form-group">
            <h5 class="col-sm-2 control-label">Policy Type</h5>
            <div class="col-sm-10">
                <select class="form-control"> 
                    <option>--Select</option>
                </select>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div style="background-color:#808080;" class="panel"></div>
        </div>
        <div class="col-md-4">
            <div style="background-color:#00ff90;" class="panel"></div>
        </div>
        <div class="col-md-6">
            <div style="background-color:#4800ff;" class="panel"></div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <form class="form-horizontal">
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Agent ID</h5>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Agebt ID">
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Copies</h5>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Copies">
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Authorization</h5>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Authorization">
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Eligibilities</h5>
                <div class="col-sm-10">
                    <textarea class="form-control" placeholder="Eligibilities"></textarea>
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Includes</h5>
                <div class="col-sm-10">
                    <textarea class="form-control" placeholder="Includes"></textarea>
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Condition</h5>
                <div class="col-sm-10">
                    <textarea class="form-control" placeholder="Conditions"></textarea>
                </div>
            </div>
            <div class="form-group">
                <h5 class="col-sm-2 control-label">Coverage Type</h5>
                <div class="col-sm-10">
                    <select class="form-control">
                        <option>-Select-</option>
                    </select>
                </div>
            </div>
            <div class="form-group" style="background-color:#dcdcdc;padding:10px;">
                <h3 class="col-sm-6 control-label" style="color:#ff0000;">Coverage Value</h3>
                <div class="col-sm-6">
                    <input type="text" class="form-control" placeholder="Coverage Value">
                </div>
            </div>
            <div class="form-group" style="background-color:#dcdcdc;padding:10px;">
                <h3 class="col-sm-6 control-label" style="color:#0026ff;">Total Policy Value</h3>
                <div class="col-sm-6">
                    <input type="text" class="form-control" placeholder="Policy Value">
                </div>
            </div>
        </form>
    </div>
</div>

